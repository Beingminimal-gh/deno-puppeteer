{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "deno",
			"command": "run",
			"problemMatcher": [
				"$deno"
			],
			"dependsOn": [
				"deno: lint"
			],
			"label": "deno: run"
		},
		{
			"type": "deno",
			"command": "test",
			"problemMatcher": [
				"$deno-test"
			],
			"dependsOn": [
				"deno: lint"
			],
			"group": "test",
			"label": "deno: test"
		},
		{
			"type": "deno",
			"command": "lint",
			"problemMatcher": [
				"$deno-lint"
			],
			"group": "test",
			"label": "deno: lint"
		}
	]
}
